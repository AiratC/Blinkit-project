{
   "version": 2,
   "name": "Blinkit",
   "builds": [
      {
         "src": "./server.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/"
      }
   ],
   "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
   ]
}
